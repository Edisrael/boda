				let invitado = null;

				if (!codigo) {
					mensajeDiv.innerHTML = "C√≥digo inv√°lido o faltante.";
				} else {
					fetch(`${scriptURL}?cd=${codigo}`)
						.then((r) => r.json())
						.then((data) => {
							if (data.error) {
								mensajeDiv.innerHTML =
									"Invitado no encontrado.";
							} else {
								invitado = data;

								// Si ya hab√≠a confirmado antes
